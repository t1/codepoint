#!/usr/bin/env java -cp ../../target/codepoint-1.0.0-SNAPSHOT-java.jar --source 11

import com.github.t1.codepoint.CodePoint;

public class Main {
    public static void main(String[] args) {
        System.out.println("A    : " + CodePoint.of("A").getInfo());
        System.out.println("range: " + CodePoint.of('A').rangeTo(CodePoint.of('C')).contains(CodePoint.of('B')));
    }
}
